{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "declarationDir": "../dist_dev/libs/types",
    "declaration": true,
    "target": "es2019",
    "module": "commonjs",
    "strict": true,
    "lib": [
      "es2019",
      "dom"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist_dev",
    "dist"
  ]
}
